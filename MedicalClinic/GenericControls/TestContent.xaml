<base:CustomUserControl x:Class="GenericControls.TestContent"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:GenericControls"
             xmlns:base="clr-namespace:GenericControls"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid Background="#FFC3CBC3">
        <Button x:Name="button" Content="Button" Margin="10,73,0,0" VerticalAlignment="Top" Click="button_Click" HorizontalAlignment="Left" Width="75"/>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="58" Margin="10,10,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="280"/>
        <Button x:Name="back" Content="Back" Margin="113,73,0,0" VerticalAlignment="Top" Click="buttonBack_Click" HorizontalAlignment="Left" Width="75"/>

    </Grid>
</base:CustomUserControl>
