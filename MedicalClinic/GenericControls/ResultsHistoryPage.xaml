<UserControl x:Class="GenericControls.ResultsHistoryPage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:GenericControls"
             mc:Ignorable="d"
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <Label x:Name="labelResultsMsg" Content="No results found." Margin="0,0,0,10" FontSize="14.667" FontStyle="Italic" Visibility="Hidden" />
        <DataGrid x:Name="dataGridResults" Margin="15,20,15,0" VerticalAlignment="Top" HorizontalAlignment="Left" Background="#FFC9AAAA" HorizontalGridLinesBrush="#FFD4C0C0" VerticalGridLinesBrush="#FFD4C0C0" FontSize="14.667" Visibility="Hidden" />
    </Grid>
</UserControl>